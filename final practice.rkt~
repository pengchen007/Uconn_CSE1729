(define (absolute x)
  (if (>= x 0)
      x
      (- 0 x)))
(absolute 5)
(absolute -6)
"b"
(define (convert-FtoC x)
  (* (/ 5 9) (- x 32)))
(convert-FtoC 68)
(define (convert-CtoF x)
  (+ (* (/ 9 5) x) 32))
(convert-CtoF 20)
"c"
(define (discount p d)
  (- p (* p d)))
(discount 100 0.1)
"d"
(define (tip p)
  (+ p (* p 0.15)))
(tip 100)
"e"
